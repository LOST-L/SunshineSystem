package com.heli.oa.common.log;

import com.heli.oa.common.entity.User;
import lombok.Data;

/**
 * Created with IntelliJ IDEA.
 * User: 白驹
 * Date: 2019/1/18
 * Time: 17:36
 * Description:
 */
@Data
public class LogEntity {
    private long id;
    /**
     * 请求地址
     */
    private String url;
    /**
     * 请求IP
     */
    private String ip;
    /**
     * 请求方式
     */
    private String httpMethod;
    /**
     * 请求类，方法
     */
    private String classMethod;
    /**
     * 方法参数
     */
    private String args;
    /**
     * 请求参数
     */
    private String reqParams;
    /**
     * 响应参数
     */
    private String respParams;
    /**
     * 响应时间
     */
    private long spendTime;
    /**
     * 日志类型（web、service）
     */
    private String logType;
    /**
     * 用户信息
     */
    private User user;
}
